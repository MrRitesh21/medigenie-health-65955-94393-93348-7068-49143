
{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>[^.]+)\\.MediCare\\.digiwil\\.in"
        }
      ],
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "MediCare\\.digiwil\\.in"
        }
      ],
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_BASE_DOMAIN": "MediCare.digiwil.in"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
